{
  "name": "minecraft-web-renderer",
  "version": "1.0.0",
  "description": "3D Minecraft world renderer using Three.js",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc && webpack --mode production",
    "start": "npm run build && node dist/server/index.js",
    "dev": "nodemon --watch src -e ts --exec \"npm run start\"",
    "compile": "tsc"
  },
  "dependencies": {
    "express": "^4.18.2",
    "three": "^0.160.0",
    "sharp": "^0.33.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "nodemon": "^3.0.2",
    "@types/three": "^0.160.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1"
  }
} 